        <img src="~/Images/quizIncludesQuestionsFrom.png" class="img-responsive"/>
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Primitive Types and Variables
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Operators and Expressions
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Conditional Statements
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Loops
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Arrays
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Methods
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Exceptions
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Object-Oriented Programming
    </div>
    <div class="quiz-description-category">
        <span class="glyphicon glyphicon-ok"></span>
        Data Structures
    </div>
    <div class="quiz-description-value">
        <span class="quiz-details-label"> Number of questions: </span> 20
    </div>
    <div class="quiz-description-value">
        <span class="quiz-details-label"> Level: </span> Mid-Level/Senior
    </div>
    <div class="quiz-description-value">
        <span class="quiz-details-label"> Time: </span> Unlimited
    </div>
    <div class="quiz-description-value">
        <span class="quiz-details-label"> Question type: </span> Multiple choice
    </div>
    <div class="quiz-start-test-button-container">
        <a title="C# Quiz" href="@Url.Action("Question", "Quiz", new {questionCategory = QuestionCategory.CSharp})" class="start-quiz-button"> Start Quiz </a>
