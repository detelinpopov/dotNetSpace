<div class="quiz-container-no-scroll-reveal quiz-frame" name="c#">
    <div class="quiz-header-container">
        <span class="lucida-sans-font">C# Quiz</span>
    </div>  
    <div class="quiz-description-value">
        <span class="quiz-details-label"> Type: </span> 20 random multiple choice questions
    </div>
    <div class="quiz-description-value">
        <span class="quiz-details-label"> Level: </span> Mid-Level/Senior
    </div>
    <div class="quiz-description-value">
        <span class="quiz-details-label"> Time: </span> Unlimited
    </div>
    <div class="quiz-start-test-button-container">
        <a title="C# Quiz" href="@Url.Action("Question", "Quiz", new {questionCategory = QuestionCategory.CSharp})" class="start-quiz-button"> Start Quiz </a>
    </div>
</div>
